{
  "$schema": "https://mintlify.com/docs.json",
  "name": "*sincronizado*",
  "theme": "maple",
  "description": "Hyper-local development stack with remote AI execution",
  "colors": {
    "primary": "#60614E",
    "light": "#7A7B63",
    "dark": "#4A4B3A"
  },
  "logo": {
    "dark": "/logo.png",
    "light": "/logo.png"
  },
  "favicon": "/logo.png",
  "navbar": {
    "links": [
      {
        "label": "GitHub",
        "href": "https://github.com/microck/sincronizado"
      }
    ],
    "primary": {
