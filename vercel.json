{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    }
  ],
  "rewrites": [
    {
      "source": "/install.sh",
      "destination": "https://raw.githubusercontent.com/microck/sincronizado/main/scripts/setup-vps.sh"
    },
    {
      "source": "/install.ps1",
      "destination": "https://raw.githubusercontent.com/microck/sincronizado/main/scripts/install.ps1"
    },
    {
      "source": "/launcher/:path*",
      "destination": "https://raw.githubusercontent.com/microck/sincronizado/main/launcher/:path*"
    },
    {
      "source": "/llms.txt",
      "destination": "https://micrtde.mintlify.app/llms.txt"
    },
    {
      "source": "/llms-full.txt",
      "destination": "https://micrtde.mintlify.app/llms-full.txt"
    },
    {
      "source": "/sitemap.xml",
      "destination": "https://micrtde.mintlify.app/sitemap.xml"
    },
    {
      "source": "/robots.txt",
      "destination": "https://micrtde.mintlify.app/robots.txt"
    },
    {
      "source": "/mcp",
      "destination": "https://micrtde.mintlify.app/mcp"
    },
    {
      "source": "/:path*",
      "destination": "https://micrtde.mintlify.app/:path*"
    }
  ],
  "headers": [
    {
      "source": "/install.sh",
      "headers": [{ "key": "Content-Type", "value": "text/plain" }]
    },
    {
      "source": "/install.ps1",
      "headers": [{ "key": "Content-Type", "value": "text/plain" }]
    },
    {
      "source": "/launcher/(.*)",
      "headers": [{ "key": "Content-Type", "value": "text/plain" }]
    }
  ]
}
