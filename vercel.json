{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/install.sh",
      "dest": "https://raw.githubusercontent.com/microck/sincronizado/main/scripts/setup-vps.sh",
      "headers": {
        "Content-Type": "text/plain"
      }
    },
    {
      "src": "/install.ps1",
      "dest": "https://raw.githubusercontent.com/microck/sincronizado/main/scripts/install.ps1",
      "headers": {
        "Content-Type": "text/plain"
      }
    },
    {
      "src": "/launcher/(.*)",
      "dest": "https://raw.githubusercontent.com/microck/sincronizado/main/launcher/$1",
      "headers": {
        "Content-Type": "text/plain"
      }
    },
    {
      "src": "/docs",
      "status": 301,
      "headers": {
        "Location": "https://micrtde.mintlify.app/"
      }
    },
    {
      "src": "/docs/(.*)",
      "status": 301,
      "headers": {
        "Location": "https://micrtde.mintlify.app/$1"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
